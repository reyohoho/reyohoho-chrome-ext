{
  "name": "ReYohoho",
  "description": "Open ReYohoho directly from kinopoisk.ru",
  "version": "1.0.6",
  "author": "reyohoho",
  "homepage_url": "https://github.com/reyohoho/reyohoho-chrome-ext",
  "manifest_version": 3,
  "host_permissions": [
    "*://*.kinopoisk.ru/"
  ],

  "content_scripts": [{
    "matches": ["https://*.kinopoisk.ru/*", "https://*.shikimori.one/*"],
    "js": ["player.js"],
    "css": ["player.css"]
  }],
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
